{
  "name": "vue-components",
  "author": {
    "name": "丁永亚"
  },
  "version": "0.1.2",
  "description": "vue 快捷组件",
  "keywords": [
    "vue",
    "element",
    "vxe-table",
    "ui"
  ],
  "main": "lib/dyyVueComponents.umd.min.js",
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lib": "vue-cli-service build --target lib --name dyyVueComponents --dest lib components/index.js",
    "build:pages": "git subtree push --prefix dist origin gh-pages",
    "prepare": "husky install",
    "commit": "git add . && git-cz && git push"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "dingyongya-vue-markdown-loader": "^2.5.2",
    "dyy-vue-components": "^0.1.1",
    "element-ui": "^2.15.8",
    "github-markdown-css": "^5.1.0",
    "highlight.js": "^11.5.1",
    "html-loader": "^3.1.0",
    "markdown-it": "^13.0.1",
    "markdown-it-container": "^3.0.0",
    "markdown-loader": "^8.0.0",
    "register-service-worker": "^1.7.1",
    "vue": "^2.6.11",
    "vue-markdown-loader": "^2.5.0",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@commitlint/prompt-cli": "^16.2.1",
    "commitizen": "^4.2.4",
    "commitlint-config-cz": "^0.13.3",
    "cz-customizable": "^6.3.0",
    "husky": "^8.0.1",
    "@vue/cli-plugin-babel": "~4.5.15",
    "@vue/cli-plugin-eslint": "~4.5.15",
    "@vue/cli-plugin-pwa": "~4.5.15",
    "@vue/cli-plugin-router": "~4.5.15",
    "@vue/cli-plugin-vuex": "~4.5.15",
    "@vue/cli-service": "~4.5.15",
    "babel-eslint": "^10.1.0",
    "dayjs": "^1.11.2",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "sass": "~1.32.13",
    "sass-loader": "~10.1.1",
    "vue-loader": "^15.9.6",
    "vue-template-compiler": "^2.6.14"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
